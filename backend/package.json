{
  "name": "@treazrisland/backend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "create-admin": "tsx src/cli/bootstrap-admin.ts",
    "cli:create-admin": "tsx src/cli/bootstrap-admin.ts",
    "cli:generate-invite": "tsx src/cli/generateInvite.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run",
    "pretest": "prisma generate",
    "test:watch": "vitest",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:migrate:reset": "prisma migrate reset --force"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.688.0",
    "@aws-sdk/s3-request-presigner": "^3.688.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/redis": "^7.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.208.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/core": "^2.2.0",
    "@opentelemetry/exporter-prometheus": "^0.208.0",
    "@opentelemetry/instrumentation-fastify": "^0.53.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-logs": "^0.208.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv-flow": "^3.2.0",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "ioredis": "^5.8.2",
    "minio": "^8.0.6",
    "pg": "^8.13.1",
    "pino": "^9.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@prisma/config": "^7.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv-flow": "^3.3.3",
    "@types/node": "^22.19.1",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.12",
    "ioredis-mock": "^8.13.1",
    "prisma": "^7.0.0",
    "supertest": "^7.1.4",
    "testcontainers": "^11.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  }
}
